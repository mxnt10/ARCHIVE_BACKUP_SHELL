ln -s "/" "$GAMELINUX_GAMEDIR/dosdevices/z:"                                                                         && echo -e "\e[1;37;44m Drive z: Verificado \e[0m"
ln -s "$HOME" "$GAMELINUX_GAMEDIR/dosdevices/h:"                                                                     && echo -e "\e[1;37;44m Drive h: Verificado \e[0m"
ln -s "$GAMELINUX_GAMEDIR/drive_c" "$GAMELINUX_GAMEDIR/dosdevices/c:"                                                && echo -e "\e[1;37;44m Drive c: Verificado \e[0m"
ln -s "$HERE/drive_c/$GAMELINUX_GAMENAME/"* "$GAMELINUX_GAMEDIR/drive_c/$GAMELINUX_GAMENAME/"                        && echo -e "\e[1;37;44m Atalho $GAMELINUX_GAMENAME Verificado \e[0m"
ln -s "$HERE/drive_c/"* "$GAMELINUX_GAMEDIR/drive_c/"                                                                && echo -e "\e[1;37;44m Atalho drive_c Verificado \e[0m"

#ln -s "$HERE/drive_c/$GAMELINUX_GAMENAME/Languages/"* "$GAMELINUX_GAMEDIR/drive_c/$GAMELINUX_GAMENAME/Languages/"    && echo -e "\e[1;37;44m Languages Verificado \e[0m"

if
     ls "$GAMELINUX_GAMEDIR" | grep -wxE '(system.reg|user.reg|userdef.reg|.update-timestamp)'
then
     echo 'Os arquivos system.reg user.reg userdef.reg .update-timestamp J√° existem no diret√≥rio espelho'
else
     cp "$HERE/system.reg" "$HERE/user.reg" "$HERE/userdef.reg" "$HERE/.update-timestamp" "/$GAMELINUX_GAMEDIR/"
fi
##############################################################################################################################################################################################################

function fun√ß√£o_gpu_teste_select() {
if 
      GAMELINUX_GPUS_TESTE=$(zenity --height=320 --width=500 --title='Marque a GPU que deseja testar' --list --text="$INFO_DRIVERS_API_GRAFICA" --radiolist --column 'Marcar' --column 'üî≤ GPUs Drivers OpenGL Vulkan' FALSE 'üüß OpenGL' FALSE 'üîµ Intel' FALSE 'üü¢ Nvidia' FALSE 'üü¢ Nouveau' FALSE 'üî¥ Amd_Mesa' FALSE 'üî¥ Amd_GpuPro' --cancel-label=Sair --ok-label=Testar | cut -d ' ' -f 2 | tr '[:upper:]' '[:lower:]') && fun√ß√£o_teste_$GAMELINUX_GPUS_TESTE 
then
     zenity --ellipsize --question --text="<span size=\"xx-large\">Deseja Usar Gpu $GAMELINUX_GPUS_TESTE para Executar o Jogo?</span>\n\n <b></b>." && echo "$GAMELINUX_GPUS_TESTE" > "$GAMELINUX_GAMEDIR/GPU.cfg" || fun√ß√£o_gpu_teste_select
else 
     zenity --ellipsize --question --icon-name=error --text="<span size=\"xx-large\">Gpu N√£o encontrado. Deseja voltar ao menu principal?</span>\n\n <b></b>." && fun√ß√£o_gpu_teste_select || exit
     
fi
}

############################################################################################################################################################################################################
if 
     cat "$GAMELINUX_GAMEDIR/GPU.cfg" | grep -wE '(opengl|nvidia|nouveau|intel|amd_mesa|amd_gpupro)'
then
     echo 'configra√ß√£o de Gpu encontrada'
else
     fun√ß√£o_gpu_teste_select
fi
############################################################################################################################################################################################################
function fun√ß√£o_soundtrack_home() {

zenity --display="$DISPLAY" --icon-name="$GAMELINUX_ZENITY_ICON_NAME" --window-icon="$GAMELINUX_ICON" --title="$GAMELINUX_GAMENAME Soundtrack" --ok-label='Sim' --question --ellipsize --text="
<span size=\"xx-large\">
Deseja por uma c√≥pia da Soundtrack do Jogo na Home?
</span>\n\n <b></b>." && cp -r -f "$HERE/drive_c/$GAMELINUX_GAMENAME/Music" "$HOME/$GAMELINUX_GAMENAME Soundtrack" 

echo 'ok' > "$GAMELINUX_GAMEDIR/Soundtrack.cfg"
}

if 
     cat "$GAMELINUX_GAMEDIR/Soundtrack.cfg" | grep -w 'ok'
then
     echo 'Soundtrack J√° foi para a Home'
else
     fun√ß√£o_soundtrack_home
fi
############################################################################################################################################################################################################


function fun√ß√£o_executar_cfg_com_opengl() {
__GL_THREADED_OPTIMIZATIONS=1 __GL_SHADER_DISK_CACHE=1 __GL_SHADER_DISK_CACHE_SKIP_CLEANUP=1 __GL_DXVK_OPTIMIZATIONS=1 MESA_GLTHREAD=true  __GL_SHADER_DISK_CACHE_PATH="$GAMELINUX_GAMEDIR/" XDG_CACHE_HOME="$GAMELINUX_GAMEDIR/" LD_LIBRARY_PATH="${HERE}/lib:${HERE}/lib64:/lib:/lib/i386-linux-gnu:/lib/x86_64-linux-gnu:/lib32:/lib64:/libx32:/usr/lib/i386-linux-gnu:/usr/lib/x86_64-linux-gnu:/usr/lib32:/usr/lib64:/usr/libx32:${LD_LIBRARY_PATH}" XDG_DATA_DIRS="${HERE}/share:${HERE}/usr/share:/usr/share:/usr/local/share:$HOME/.local/share:${XDG_DATA_DIRS}" PATH="${HERE}/bin:$PATH" FONTCONFIG_PATH="${HERE}/etc/fonts" DISPLAY="$DISPLAY" PULSE_LATENCY_MSEC=60 WINE_LARGE_ADDRESS_AWARE=1 STAGING_SHARED_MEMORY=1 NTFS_MODE=0 WINE64PRELOADER="${HERE}/bin/wine64-preloader" WINEPRELOADER="${HERE}/bin/wine-preloader" WINEPATH="${HERE}/bin/winepath" WINEDBG="${HERE}/bin/winedbg" WINECONSOLE="${HERE}/bin/wineconsole" WINECFG="${HERE}/bin/winecfg" WINEBOOT="${HERE}/bin/wineboot" WINESERVER="${HERE}/bin/wineserver" WINDOWS_VERSION="$Versao_do_windows" WINEESYNC=1 GST_PLUGIN_PATH="$Local_dos_plugins_gstreamer" SDL_AUDIODRIVER="$Driver_de_audio_quando_usar_sdl" WINE64="${HERE}/bin/wine64" WINE="${HERE}/bin/wine" USE_SYSTEM_WINE=0 WINEDLLOVERRIDES="$Dlls_para_desabilitar=d;$Dlls_modo_build=b;$Dlls_nativas_sem_vulkan=n" WINEPREFIX="$GAMELINUX_GAMEDIR" WINEARCH="$Arquitetura_do_windows" "${HERE}/bin/wine" 'C:\\windows\\system32\\start.exe' /wait /max /b /d "$Diretorio_do_executavel_windows" "$Arquivo_exe_executavel_windows_cfg" | GAMELINUX_ZENITY_TITLE="Executando Configura√ß√µes" fun√ß√£o_zenity_executando
}

