function fun√ß√£o_verificar_depend√™ncias() {

if   
     cat "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg" | grep -w -m1 'GameLinux_Depend√™ncias_Instaladas'
then
     echo 'Depend√™ncias J√° foram instaladas por GameLinux'
elif 
     cat "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg" | grep -w -m1 'ok'
then
     echo 'Depend√™ncias J√° foram instaladas por outro software que demanda as mesmas depend√™ncias'
elif 
     dpkg -V 'lutris' 'mesa-vulkan-drivers:i386'
then
     echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     dpkg -V 'playonlinux' 'mesa-vulkan-drivers:i386'
then
     echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     dpkg -V 'q4wine' 'wine32' 'mesa-vulkan-drivers:i386'
then 
     echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     dpkg -V 'mesa-vulkan-drivers:i386' 'wine-stable:i386'
then
     echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     dpkg -V 'mesa-vulkan-drivers:i386' 'wine-development:i386'
then
     echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     dpkg -V 'mesa-vulkan-drivers:i386' 'dxvk-wine32-development:i386'
then
     echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     dpkg -V 'mesa-vulkan-drivers:i386' 'wine32-development:i386'
then
     echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     dpkg -V 'mesa-vulkan-drivers:i386' 'wine-staging:i386'
then
    echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     dpkg -V $DEPENDENCIAS_I386_COMUNS_UBUNTU_18_04_22_04_LTS
then
     echo 'GameLinux_Depend√™ncias_Instaladas' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     pacman -Qi 'zenity' 'lutris' 'lib32-mesa' 'lib32-ncurses'  'lib32-vulkan-icd-loader' 'lib32-alsa-plugins' 'lib32-gst-plugins-base-libs'
then
     echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     pacman -Qi $DEPENDENCIAS_I386_AMD64_MULTILIB_BASE_ARCH
then 
     echo 'GameLinux_Depend√™ncias_Instaladas' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     rpm -qa | grep mesa-dri-drivers | grep i686 && rpm -qa | grep mesa-vulkan-drivers | grep i686 && rpm -qa | grep lutris
then 
     echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
elif 
     rpm -qa | grep mesa-dri-drivers | grep i686 && rpm -qa | grep openldap | grep i686 && rpm -qa | grep ocl-icd | grep i686 && rpm -qa | grep pulseaudio-libs | grep i686 && rpm -qa | grep alsa-lib | grep i686 && rpm -qa | grep ncurses-libs | grep i686 && rpm -qa | grep libX11 | grep i686 && rpm -qa | grep libXrandr | grep i686 && rpm -qa | grep mesa-vulkan-drivers | grep i686 && rpm -qa | grep vulkan-loader | grep i686 && rpm -qa | grep mesa-libGL | grep i686 && rpm -qa | grep gvfs | grep i686 && rpm -qa | grep SDL2 | grep i686 && rpm -qa | grep SDL_ttf | grep i686 && rpm -qa | grep SDL_net | grep i686 && rpm -qa | grep SDL_sound | grep i686 && rpm -qa | grep SDL_image | grep i686 && rpm -qa | grep SDL_Pango | grep i686 && rpm -qa | grep libwayland-egl | grep i686 && rpm -qa | grep libXcomposite | grep i686 && rpm -qa | grep libXdamage | grep i686 && rpm -qa | grep libjpeg-turbo | grep i686 && rpm -qa | grep libpng | grep i686 && rpm -qa | grep zlib | grep i686 && rpm -qa | grep krb5-libs | grep i686 && rpm -qa | grep nss | grep i686 && rpm -qa | grep libFAudio | grep i686 && rpm -qa | grep pipewire | grep i686 && rpm -qa | grep pipewire-libs | grep i686 && rpm -qa | grep openssl-libs | grep i686 && rpm -qa | grep mesa-libOSMesa | grep i686 && rpm -qa | grep mesa-libglapi | grep i686 && rpm -qa | grep zenity
then 
     echo 'GameLinux_Depend√™ncias_Instaladas' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
else 
     clear && echo 'Dependencias n√£o encontradas' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
fi
}

##################################################################################################################################
###############  Fim do Bloco de fun√ß√µes para depend√™ncias  ####################################################################
#
#üü•fun√ß√£o_zenity_executando
#  Barra de progresso Executando
#
#üü•fun√ß√£o_zenity_dependencias_i386_n√£o_encontradas_pergunta
# Mensagem de erro se depend√™ncias n√£o for encontradas. pergunta se deseja instalar.
#
#üü•fun√ß√£o_zenity_dependencias_i386_n√£o_encontradas
# Mensagem de erro se depend√™ncias n√£o for encontradas.
# 
#üü•fun√ß√£o_zenity_barra_de_progresso_pulssante
# Barra de progresso. Vari√°veis de t√≠tulo,texto,√≠cone da janela,nome do √≠cone, podem ser definidas antes de executar.
#
#üü•fun√ß√£o_verificar_depend√™ncias
# Fun√ß√£o que verifica se as depend√™ncias necess√°rias est√£o instaladas
#
#üü•fun√ß√£o_instalar_depend√™ncias_i386_amd64_sistemas_base_ubuntu_debian
# Fun√ß√£o de instalar as depend√™ncias em sistemas debian ou ubuntu e distros baseados nesses sistemas. 
#
#üü•fun√ß√£o_instalar_depend√™ncias_i386_amd64_sistemas_base_arch
# Fun√ß√£o de instalar as depend√™ncias em sistemas arch e distros baseados nesses sistemas. 
#
#üü•fun√ß√£o_instalar_depend√™ncias_i386_amd64_fedora
# Fun√ß√£o de instalar as depend√™ncias em sistemas fedora. 
#
###################################################################################################################################

if    
      cat "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg" | grep -wE '(GameLinux_Depend√™ncias_Instaladas|ok)'
then
      echo 'Depend√™ncias encontradas'      
elif      
      echo "$GAMELINUX_CONSULTA_OS_RELEASE_NOME $GAMELINUX_CONSULTA_OS_RELEASE_BASE" | grep -wiE -m1 '(ubuntu|debian|zorin|pop)'
then
      fun√ß√£o_verificar_depend√™ncias | GAMELINUX_ZENITY_TEXT='Verificando Depend√™ncias' fun√ß√£o_zenity_barra_de_progresso_pulssante 
      cat "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg" | grep -wE '(GameLinux_Depend√™ncias_Instaladas|ok)' || fun√ß√£o_zenity_dependencias_i386_n√£o_encontradas_pergunta || exit 
      cat "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg" | grep -wE '(GameLinux_Depend√™ncias_Instaladas|ok)' || fun√ß√£o_instalar_depend√™ncias_i386_amd64_sistemas_base_ubuntu_debian | fun√ß√£o_zenity_barra_de_progresso_pulssante
      
elif  
      echo "$GAMELINUX_CONSULTA_OS_RELEASE_NOME $GAMELINUX_CONSULTA_OS_RELEASE_BASE" | grep -wiE -m1 '(manjaro|biglinux)'
then
     fun√ß√£o_verificar_depend√™ncias | GAMELINUX_ZENITY_TEXT='Verificando Depend√™ncias' fun√ß√£o_zenity_barra_de_progresso_pulssante 
      cat "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg" | grep -wE '(GameLinux_Depend√™ncias_Instaladas|ok)' || fun√ß√£o_zenity_dependencias_i386_n√£o_encontradas_pergunta || exit 
      cat "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg" | grep -wE '(GameLinux_Depend√™ncias_Instaladas|ok)' || fun√ß√£o_instalar_depend√™ncias_i386_amd64_sistemas_base_arch | fun√ß√£o_zenity_barra_de_progresso_pulssante

elif  
      echo "$GAMELINUX_CONSULTA_OS_RELEASE_NOME $GAMELINUX_CONSULTA_OS_RELEASE_BASE" | grep -wiE -m1 '(fedora|Fedora Linux 36)'
then
     fun√ß√£o_verificar_depend√™ncias | GAMELINUX_ZENITY_TEXT='Verificando Depend√™ncias' fun√ß√£o_zenity_barra_de_progresso_pulssante 
      cat "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg" | grep -wE '(GameLinux_Depend√™ncias_Instaladas|ok)' || fun√ß√£o_zenity_dependencias_i386_n√£o_encontradas_pergunta || exit 
      cat "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg" | grep -wE '(GameLinux_Depend√™ncias_Instaladas|ok)' || fun√ß√£o_instalar_depend√™ncias_i386_amd64_fedora | fun√ß√£o_zenity_barra_de_progresso_pulssante  
elif  
      echo "$GAMELINUX_CONSULTA_OS_RELEASE_NOME $GAMELINUX_CONSULTA_OS_RELEASE_BASE" | grep -wiE -m1 '(Regata OS|regataos)' && echo 'Bypass'
then
     echo 'ok' > "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg"
else
      echo 'deu merda!'
fi # terminal o trecho de c√≥digo
###########################################################################################################################################################################################

cat "$GAMELINUX_DIRETORIO_CONFIG/Depend√™ncias.cfg" | grep -wE '(GameLinux_Depend√™ncias_Instaladas|ok)' || fun√ß√£o_zenity_dependencias_i386_n√£o_encontradas 

function fun√ß√£o_teste_opengl() { 
timeout 3 "${HERE}/bin/glxgears_i386" -info -geometry 600x500 -display "$DISPLAY" | grep -w 'GL_VENDOR'
}

function fun√ß√£o_teste_nouveau() { 
DRI_PRIME=1 timeout 3 "${HERE}/bin/glxgears_i386" -info -geometry 600x500 -display "$DISPLAY" | grep -w 'GL_VENDOR' | cut -d '=' -f2 | grep 'nouveau'
}

function fun√ß√£o_teste_nvidia() { 
VK_ICD_NVIDIA=$(ls /usr/share/vulkan/icd.d/*nvidia*.json /etc/vulkan/icd.d/*nvidia*.json | grep 'nvidia' | paste -d: -s )
__NV_PRIME_RENDER_OFFLOAD_PROVIDER=NVIDIA-G0 __NV_PRIME_RENDER_OFFLOAD=1 __GLX_VENDOR_LIBRARY_NAME=nvidia EGL_NV_triple_buffer=1 __VK_LAYER_NV_optimus=NVIDIA VK_ICD_FILENAMES="$VK_ICD_NVIDIA" "${HERE}/bin/vulkan-smoketest_i386" --w 600 --h 500 --c 300
}

function fun√ß√£o_teste_intel() { 
VK_ICD_INTEL=$(ls /usr/share/vulkan/icd.d/*intel*.json /etc/vulkan/icd.d/*intel*.json | grep 'intel' | paste -d: -s )
VK_ICD_FILENAMES="$VK_ICD_INTEL" "${HERE}/bin/vulkan-smoketest_i386" --w 600 --h 500 --c 700
}

function fun√ß√£o_teste_amd_mesa() { 
VK_ICD_AMD_MESA=$(ls /usr/share/vulkan/icd.d/*radeon*.json /etc/vulkan/icd.d/*radeon*.json | grep 'radeon' | paste -d: -s )
VK_ICD_FILENAMES="$VK_ICD_AMD_MESA" "${HERE}/bin/vulkan-smoketest_i386" --w 600 --h 500 --c 300
}

function fun√ß√£o_teste_amd_gpupro() {
VK_ICD_AMD_GPUPRO=$(ls /opt/amdgpu-pro/etc/vulkan/icd.d/*amd*.json /opt/amdgpu-pro/etc/vulkan/icd.d/*amd*.json /etc/vulkan/icd.d/*amd*.json | grep 'amd' | paste -d: -s )
VK_ICD_FILENAMES="$VK_ICD_AMD_GPUPRO" "${HERE}/bin/vulkan-smoketest_i386" --w 600 --h 500 --c 300
}

